{
  "experiment_name": "DCGAN_smooth_labels",
  "seed": 2,
  "model_params": {
    "discriminator": {
      "ndf": 64
    },
    "generator": {
      "nz": 100,
      "ngf": 64
    }
  },
  "optimizer_params": {
    "discriminator": {
      "name": "AdamW",
      "params": {
      "betas": [
        0.5,
        0.999
      ],
      "lr": 2e-4,
      "weight_decay": 0.01
      }
    },
    "generator": {
      "name": "AdamW",
      "params": {
      "betas": [
        0.5,
        0.999
      ],
      "lr": 2e-4,
      "weight_decay": 0.01
      }
    }
  },
  "scheduler_params": {
    "discriminator": {
      "name": "ExponentialLR",
      "params": {
        "gamma": 0.9
      }
    },
    "generator": {
      "name": "ExponentialLR",
      "params": {
        "gamma": 0.9
      }
    }
  },
  "epochs": 25,
  "warmup_epochs": 5,
  "batch_size": 128,
  "checkpoint_folder": "./checkpoints/DCGAN_smooth_labels",
  "image_size": 64,
  "early_stopping": {
    "patience": 10,
    "min_delta": 0.001
  }
}